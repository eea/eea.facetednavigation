   <property name="icon_expr"></property>
   <property
      name="available_expr">object/@@faceted_search_subtyper/can_disable</property>
   <property name="permissions">
    <element value="eea.facetednavigation: Configure faceted"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="faceted.search.enable" meta_type="CMF Action" i18n:domain="eea">
   <property name="title"
      i18n:translate="">Enable faceted search</property>
