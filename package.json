{
    "name": "eea.facetednavigation",
    "version": "15.0.0",
    "author": "European Environment Agency: IDM2 A-Team <eea-edw-a-team-alerts@googlegroups.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/eea/eea.facetednavigation"
    },
    "license": "MIT",
    "engines": {
        "node": ">=8.12.0",
        "yarn": "^1.3.2"
    },
    "dependencies": {
        "jquery2-bbq": "1.3.1",
        "jquery.cookie": "1.4.1",
        "select2": "3.5.1"
    },
    "devDependencies": {
        "clean-css-cli": "^5.6.0",
        "copy-webpack-plugin": "^10.2.4",
        "core-js": "^3.21.1",
        "css-loader": "^6.7.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint": "^8.13.0",
        "expose-loader": "^3.1.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "style-loader": "^3.3.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1",
        "webpack": "^5.72.0"
    },
    "resolutions": {
        "jquery": "3.6.0"
    },
    "resolutions-comments": {
        "ua-parser-js": "See https://github.com/faisalman/ua-parser-js/issues/536"
    },
    "scripts": {
        "build": "npm-run-all build:webpack build:css",
        "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
        "build:css": "npm-run-all css-minify-plugins css-minify-view css-minify-edit",
        "watch:webpack": "NODE_ENV=development webpack --config webpack.config.js --watch",
        "start": "NODE_ENV=development webpack serve --config webpack.config.js",
        "css-minify-plugins": "cleancss --format breakWith=lf --source-map --source-map-inline-sources --output eea/facetednavigation/browser/static/faceted-jquery.min.css ./resources/plugins.css",
        "css-minify-view": "cleancss --format breakWith=lf --source-map --source-map-inline-sources --output eea/facetednavigation/browser/static/faceted-view.min.css ./resources/view.css",
        "css-minify-edit": "cleancss --format breakWith=lf --source-map --source-map-inline-sources --output eea/facetednavigation/browser/static/faceted-edit.min.css ./resources/edit.css"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ]
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/eea/eea.facetednavigation"
}
