   <property name="permissions">
    <element value="eea.facetednavigation: Configure faceted"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="faceted.search.disable" meta_type="CMF Action" i18n:domain="eea">
   <property name="title"
      i18n:translate="">Disable faceted search</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object/absolute_url}/@@faceted_search_subtyper/disable</property>
